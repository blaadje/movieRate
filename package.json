{
  "name": "electron-react",
  "version": "0.0.1",
  "author": "Charlot Alexandre <acharlot91@gmail.com>",
  "description": "Application for rate movies",
  "main": "src/main.js",
  "scripts": {
    "start": "NODE_ENV=development concurrently \"webpack-dev-server --config=./config/dev/webpack.config.js --hot --inline --mode development\" \"sleep 2 && electron .\"",
    "build": "node ./config/build.js && electron-builder",
    "lint": "tslint -c tslint.json --project tsconfig.json --fix",
    "test": "jest"
  },
  "jest": {
    "modulePaths": [
      "src"
    ],
    "moduleNameMapper": {
      "^components$": "<rootDir>/src/components/",
      "^containers$": "<rootDir>/src/container/",
      "^core$": "<rootDir>/src/core/",
      ".+\\.(css|styl|svg|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "build": {
    "productName": "Mon application",
    "appId": "org.simulatedgreg.electron-vue",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "target": [
        "squirrel"
      ],
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.2",
    "@types/react": "^16.4.7",
    "@types/react-onclickoutside": "^6.0.3",
    "@types/react-redux": "^6.0.6",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-test-renderer": "^16.0.1",
    "@types/redux-logger": "^3.0.6",
    "@types/redux-orm": "^0.9.0",
    "@types/urijs": "^1.15.38",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2017-node7": "^0.5.2",
    "babel-preset-react": "^6.24.1",
    "cfonts": "^1.2.0",
    "chalk": "^2.3.2",
    "concurrently": "^3.6.1",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "electron": "^1.8.4",
    "electron-builder": "^20.10.0",
    "electron-builder-squirrel-windows": "^20.10.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^12.0.1",
    "electron-prebuilt": "^1.4.13",
    "electron-prebuilt-compile": "^1.8.4",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "multispinner": "^0.2.1",
    "node-sass": "^4.9.3",
    "react-hot-loader": "^4.3.4",
    "react-test-renderer": "^16.3.1",
    "redux-saga-thunk": "^0.7.1",
    "regenerator-runtime": "^0.11.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.2.0",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^7.1.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^14.0.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "emotion": "^9.1.2",
    "lodash": "^4.17.10",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-emotion": "^9.1.2",
    "react-inlinesvg": "^0.8.1",
    "react-load-image": "^0.1.6",
    "react-onclickoutside": "^6.7.1",
    "react-popper": "^1.0.0-beta.5",
    "react-rating": "^1.1.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-orm": "^0.12.1",
    "redux-saga": "^0.16.0",
    "urijs": "^1.19.1"
  }
}
