{
  "name": "electron-react",
  "version": "0.0.1",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "start": "NODE_ENV=development concurrently \"webpack-dev-server --config=./config/webpack.config.js --hot --inline --mode development\" \"sleep 2 && electron .\"",
    "prod": "webpack --config=webpack.config.js",
    "build": "node ./config/build.js && electron-builder",
    "lint": "eslint --ext .js,.tsx src",
    "lint:fix": "eslint --ext .js,.tsx --fix src"
  },
  "build": {
    "productName": "Mon application",
    "appId": "org.simulatedgreg.electron-vue",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "devDependencies": {
    "@types/node": "^9.6.2",
    "@types/react": "^16.3.5",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "cfonts": "^1.2.0",
    "chalk": "^2.3.2",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1",
    "electron-devtools-installer": "^2.2.3",
    "electron-packager": "^11.1.0",
    "electron-prebuilt": "^1.4.13",
    "electron-prebuilt-compile": "^1.8.3",
    "eslint": "^4.19.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "multispinner": "^0.2.1",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "ts-loader": "^4.2.0",
    "typescript": "^2.8.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
